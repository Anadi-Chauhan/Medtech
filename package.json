{
  "name": "medtech",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"cross-env PAYLOAD_CONFIG_PATH=payload.config.js nodemon\" \"next dev\" ",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=payload.config.js payload build",
    "build:next": "next build",
    "build:server": "node server.js",
    "build": "npm run copyfiles && npm run build:payload && npm run build:next",
    "serve": "concurrently \"cross-env PAYLOAD_CONFIG_PATH=payload.config.js NODE_ENV=production node server.js\" \"next start\"",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" /",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=payload.config.js payload generate:graphQLSchema",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=payload.config.js payload",
    "start": "concurrently \"node app.js\" \"next start\"",
    "lint": "next lint"
  },
